{
  "name": "levelgraph-jsonld",
  "version": "0.3.0",
  "description": "The Object Document Mapper for LevelGraph based on JSON-LD",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --recursive test",
    "zuul": "zuul -- test",
    "coverage": "rm -rf coverage; istanbul cover _mocha -- --recursive --reporter spec --bail",
    "publish-coverage": "cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcollina/levelgraph-jsonld.git"
  },
  "bugs": {
    "url": "http://github.com/mcollina/levelgraph-jsonld/issues"
  },
  "keywords": [
    "object",
    "document",
    "mapper",
    "odm",
    "graph",
    "graph",
    "database",
    "json",
    "json-ld",
    "rdf",
    "linked data"
  ],
  "author": "Matteo Collina <hello@matteocollina.com>",
  "license": "MIT",
  "dependencies": {
    "jsonld": "git://github.com/elf-pavlik/jsonld.js.git#browserify",
    "uuid": "~1.4.1",
    "async": "~0.2.9",
    "n3": "~0.2.3"
  },
  "peerDependencies": {
    "levelgraph": "~0.8.1"
  },
  "devDependencies": {
    "mocha": "~1.17.0",
    "levelgraph": "~0.8.1",
    "level-test": "~1.6.0",
    "chai": "~1.8.1",
    "lodash": "~2.4.1",
    "coveralls": "~2.6.1",
    "istanbul": "~0.2.3",
    "zuul": "~1.5.2",
    "browserify": "~3.20.0",
    "uglify-js": "~2.4.10"
  }
}
